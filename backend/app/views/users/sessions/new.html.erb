<% content_for :breadcrumbs do %>
  <a href="#">Home</a>
  Login
<% end %>

<% content_for :help do %>
  <h3>Help For Login</h3>
  <p>This is login screen. Here we can login by entering username and password </p>
<% end %>


<div class="contentmain">
  <div class="imagemain">
    <div id="carousel-example-generic" class="carousel slide" data-interval="3000" data-ride="carousel">
      <div class="carousel-inner">
        <div class="item active">
          <img src="/assets/tank1.jpg" alt="Slide Image 1">
        </div>
        <div class="item">
          <img src="/assets/tank2.jpg" alt="Slide Image 2">
        </div>
        <div class="item">
          <img src="/assets/tank3.jpg" alt="Slide Image 3">
        </div>
        <div class="item">
          <img src="/assets/indianarmy1.jpg" alt="Slide Image 4">
        </div>
        <div class="item">
          <img src="/assets/indianarmy2.jpg" alt="Slide Image 5">
        </div>
        <div class="item">
          <img src="/assets/indianarmy3.jpg" alt="Slide Image 6">
        </div>
        <div class="item">
          <img src="/assets/indianarmy4.png" alt="Slide Image 7">
        </div>
      </div>
    </div>

  </div>
  <div class="loginmain">
    <h3>LOGIN</h3>
    <hr/>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <h5>&nbsp;Mus Number</h5>
      <%= f.text_field :uid, autofocus: true %>

      <h5>&nbsp;Password</h5>
      <%= f.password_field :password, autocomplete: "off" %>

      <% if devise_mapping.rememberable? -%>
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      <% end -%>
      <br/><br/>
      <%= f.submit "Sign in", :class => "login-button btn btn-primary" %>
    <% end %>
    <div class="forgot-password">
      <%= render "devise/shared/links" %>    <br/><br/>
    </div>
  </div>
</div>

<div class="footer-main">
  <div>
